 {
     "builds": [
         {
             "src": "api/index.py",
             "use": "@vercel/python"
         }
     ],
     "routes": [
         {
             "src": "/(.*)",
             "dest": "api/index.py"
         },
          {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
     ]
 }
