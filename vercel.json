{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
         { "src": "*.js", "use": "@vercel/node" },
          { "src": "*.html", "use": "@vercel/static" },
           { "src": "*.py", "use": "@vercel/python" }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        }
    ]
}